        "gray": {
          "50": "#fafafa",
          "100": "#f5f5f5",
          "200": "#e5e5e5",
          "300": "#d4d4d4",
          "400": "#a3a3a3",
          "500": "#737373",
          "700": "#404040",
          "800": "#262626",
          "900": "#171717"
        },
        "neutral": {
          "100": "#f5f5f5",
          "200": "#e5e5e5",
          "300": "#d4d4d4",
          "400": "#a3a3a3",
          "500": "#737373",
          "800": "#262626",
          "900": "#171717"
        },
        "zinc": {
          "50": "#fafafa",
          "200": "#e4e4e7",
          "300": "#d4d4d8",
          "700": "#3f3f46"
        }
      },
      "accent": {
        "red": {
          "500": "#ef4444",
          "600": "#dc2626",
          "700": "#b91c1c",
          "custom": {
            "3B3B": "#FF3B3B",
            "5722": "#FF5722",
            "E64A19": "#E64A19",
            "8B2C1C": "#8B2C1C",
            "2A0F0A": "#2A0F0A"
          },
          "orange": {
            "4500": "#FF4500"
          }
        }
      }
