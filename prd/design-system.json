    "typography": {
      "fontFamilies": {
        "primary": {
          "name": "Outfit",
          "fallback": "Arial, sans-serif",
          "variable": "--font-outfit"
        },
        "secondary": {
          "name": "NyghtSerif",
          "fallback": "Arial, serif",
          "variable": "--font-nyght-serif",
          "style": "italic"
        },
        "display": {
          "name": "Geist",
          "fallback": "Arial, sans-serif",
          "variable": "--font-geist-sans"
        },
        "mono": {
          "name": "Geist Mono",
          "fallback": "Arial, monospace",
          "variable": "--font-geist-mono"
        },
        "accent": {
          "name": "agile",
          "fallback": "Arial, sans-serif",
          "variable": "--font-agile"
        }
      },
      "fontSizes": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem",
        "42px": "2.625rem",
        "44px": "2.75rem",
        "48px": "3rem",
        "54px": "3.375rem",
        "58px": "3.625rem",
        "5xl": "3rem",
        "6xl": "3.75rem"
      },
      "fontWeights": {
        "light": 300,
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "lineHeights": {
        "tight": 1.25,
        "relaxed": 1.625,
        "wide": 2
      },
      "letterSpacing": {
        "wide": "0.025em",
        "wider": "0.05em",
        "widest": "0.1em",
        "custom": "0.2em"
      }
    },

    "spacing": {
      "scale": "tailwind",
      "values": {
        "0": "0",
        "1": "0.25rem",
        "2": "0.5rem",
        "3": "0.75rem",
        "4": "1rem",
        "6": "1.5rem",
        "8": "2rem",
        "10": "2.5rem",
        "12": "3rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem",
        "32": "8rem",
        "40": "10rem"
      },
      "gaps": {
        "xs": "0.5rem",
        "sm": "0.75rem",
        "md": "1rem",
        "lg": "1.5rem",
        "xl": "2rem",
        "2xl": "2.5rem",
        "3xl": "3rem"
      },
      "padding": {
        "xs": "0.25rem 0.5rem",
        "sm": "0.5rem 1rem",
        "md": "1rem 1.5rem",
        "lg": "1.5rem 2rem",
        "xl": "2rem 3rem",
        "button": {
          "small": "0.5rem 1rem",
          "medium": "0.625rem 1rem",
          "large": "0.75rem 1.5rem"
        }
      },
      "margins": {
        "xs": "0.5rem",
        "sm": "1rem",
        "md": "1.5rem",
        "lg": "2rem",
        "xl": "3rem",
        "2xl": "4rem",
        "3xl": "5rem"
      }
    },

    "borderRadius": {
      "none": "0",
      "sm": "0.125rem",
      "md": "0.375rem",
      "lg": "0.5rem",
      "xl": "0.75rem",
      "2xl": "1rem",
      "3xl": "1.5rem",
      "full": "9999px",
      "custom": {
        "20px": "20px"
      }
    },

    "shadows": {
      "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
      "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)",
      "custom": {
        "insetRed": "inset 0 -20px 80px -20px rgba(255, 100, 100, 0.12)",
        "whiteGlow": "0 0 3px rgba(255, 255, 255, 0.7)",
        "textShadow": "0px 4px 8px rgba(255, 255, 255, 0.05), 0px 8px 30px rgba(255, 255, 255, 0.25)",
        "cardInset": "rgb(255, 255, 255) 0px 2px 20px inset, rgba(255, 255, 255, 0.49) 0px -10px 50px 1px"
      }
    },

    "effects": {
      "backdropBlur": {
        "xs": "blur(2px)",
        "sm": "blur(4px)",
        "md": "blur(8px)",
        "lg": "blur(16px)",
        "xl": "blur(24px)",
        "2xl": "blur(40px)",
        "3xl": "blur(64px)",
        "custom": {
          "150px": "blur(150px)",
          "80px": "blur(80px)"
        }
      },
      "opacity": {
        "05": "0.05",
        "10": "0.1",
        "20": "0.2",
        "30": "0.3",
        "40": "0.4",
        "50": "0.5",
        "60": "0.6",
        "70": "0.7",
        "75": "0.75",
        "80": "0.8",
        "85": "0.85",
        "90": "0.9",
        "95": "0.95"
      },
      "gradients": {
        "radial": "radial-gradient(circle, var(--from), var(--to))",
        "linear": {
          "toRight": "linear-gradient(to right, var(--from), var(--to))",
          "toBottom": "linear-gradient(to bottom, var(--from), var(--to))",
          "toLeft": "linear-gradient(to left, var(--from), var(--to))",
          "diagonal": "linear-gradient(135deg, var(--from), var(--to))",
          "text": "linear-gradient(to bottom, var(--from), var(--via), var(--to))"
        }
      },
      "animations": {
        "duration": {
          "fast": "150ms",
          "normal": "300ms",
          "slow": "500ms",
          "slower": "1000ms"
        },
        "easing": {
          "easeIn": "cubic-bezier(0.4, 0, 1, 1)",
          "easeOut": "cubic-bezier(0, 0, 0.2, 1)",
          "easeInOut": "cubic-bezier(0.4, 0, 0.2, 1)",
          "easeInOutAlt": "cubic-bezier(0.4, 0, 0.2, 1)"
        }
      }
    },

    "layout": {
      "container": {
        "maxWidth": {
          "sm": "640px",
          "md": "768px",
          "lg": "1024px",
          "xl": "1280px",
          "2xl": "1408px",
          "3xl": "1536px",
          "full": "100%"
        },
        "padding": {
          "mobile": "1rem",
          "tablet": "1.5rem",
          "desktop": "2rem",
          "xl": "2rem"
        }
      },
      "breakpoints": {
        "xs": "475px",
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px",
        "2xl": "1536px"
      },
      "grid": {
        "columns": {
          "1": "repeat(1, minmax(0, 1fr))",
          "2": "repeat(2, minmax(0, 1fr))",
          "3": "repeat(3, minmax(0, 1fr))",
          "4": "repeat(4, minmax(0, 1fr))",
          "12": "repeat(12, minmax(0, 1fr))"
        },
        "gap": {
          "sm": "0.5rem",
          "md": "1rem",
          "lg": "1.5rem",
          "xl": "2rem"
        }
      }
    },

    "components": {
      "button": {
        "primary": {
          "backgroundColor": "rgba(255, 255, 255, 0.1)",
          "border": "1px solid rgba(255, 255, 255, 0.1)",
          "borderRadius": "9999px",
          "padding": "0.625rem 1rem",
          "color": "#ededed",
          "fontFamily": "Outfit",
          "fontSize": "0.875rem",
          "fontWeight": 500,
          "transition": "all 300ms ease-in-out",
          "hover": {
            "backgroundColor": "transparent",
            "color": "#000000"
          }
        },
        "secondary": {
          "backgroundColor": "rgba(255, 255, 255, 0.15)",
          "border": "1px solid rgba(255, 255, 255, 0.1)",
          "borderRadius": "9999px",
          "padding": "0.625rem 1.5rem",
          "color": "#ffffff",
          "fontFamily": "Outfit",
          "fontSize": "0.875rem",
          "fontWeight": 400,
          "transition": "all 300ms ease-in-out",
          "hover": {
            "backgroundColor": "rgba(255, 255, 255, 0.2)"
          }
        }
      },
      "card": {
        "backgroundColor": "#0a0a0a",
        "border": "1px solid rgba(255, 255, 255, 0.1)",
        "borderRadius": "20px",
        "padding": "1.5rem",
        "backdropFilter": "blur(2px)",
        "boxShadow": "inset 0 -20px 80px -20px rgba(255, 100, 100, 0.12)",
        "transition": "all 300ms ease-in-out",
        "hover": {
          "borderColor": "rgba(255, 255, 255, 0.2)",
          "boxShadow": "inset 0 -20px 80px -20px rgba(255, 100, 100, 0.2)"
        }
      },
      "navigation": {
        "backgroundColor": "rgba(0, 0, 0, 0.2)",
        "backdropFilter": "blur(8px)",
        "border": "1px solid rgba(255, 255, 255, 0.1)",
        "borderRadius": "9999px",
        "padding": "0.25rem 0.375rem",
        "link": {
          "color": "rgba(255, 255, 255, 0.7)",
          "fontSize": "0.875rem",
          "fontWeight": 300,
          "padding": "0.375rem 1rem",
          "borderRadius": "9999px",
          "transition": "color 300ms ease-in-out",
          "hover": {
            "color": "rgba(255, 255, 255, 0.85)"
          },
          "active": {
            "color": "#ffffff",
            "backgroundColor": "rgba(255, 255, 255, 0.05)"
          }
        }
      },
      "input": {
        "backgroundColor": "rgba(255, 255, 255, 0.05)",
        "border": "1px solid rgba(255, 255, 255, 0.1)",
        "borderRadius": "0.5rem",
        "padding": "0.75rem 1rem",
        "color": "#ededed",
        "fontFamily": "Outfit",
        "fontSize": "1rem",
        "transition": "all 300ms ease-in-out",
        "focus": {
          "borderColor": "rgba(255, 255, 255, 0.3)",
          "outline": "none",
          "boxShadow": "0 0 0 3px rgba(255, 255, 255, 0.1)"
        }
      },
      "badge": {
        "backgroundColor": "#ef4444",
        "color": "#ffffff",
        "borderRadius": "0.5rem",
        "padding": "0.25rem 0.625rem",
        "fontSize": "0.75rem",
        "fontWeight": 600,
        "backdropFilter": "blur(4px)",
        "border": "1px solid rgba(255, 255, 255, 0.1)"
      }
    },

    "patterns": {
      "glassmorphism": {
        "backgroundColor": "rgba(255, 255, 255, 0.05)",
        "backdropFilter": "blur(8px)",
        "border": "1px solid rgba(255, 255, 255, 0.1)"
      },
      "gradientText": {
        "background": "linear-gradient(to bottom, #ffffff, rgba(255, 255, 255, 0.6), #a3a3a3)",
        "backgroundClip": "text",
        "WebkitBackgroundClip": "text",
        "WebkitTextFillColor": "transparent",
        "color": "transparent"
      },
      "glowEffect": {
        "boxShadow": "0 0 15px rgba(255, 69, 0, 0.4)"
      },
      "blurOverlay": {
        "position": "absolute",
        "inset": 0,
        "backgroundColor": "rgba(255, 255, 255, 0.05)",
        "backdropFilter": "blur(2px)"
      }
    },

    "transitions": {
      "default": "all 300ms cubic-bezier(0.4, 0, 0.2, 1)",
      "colors": "color 300ms ease-in-out",
      "transform": "transform 300ms ease-in-out",
      "opacity": "opacity 300ms ease-in-out",
      "scale": "scale 300ms ease-in-out"
    },

    "zIndex": {
      "base": 0,
      "dropdown": 10,
      "sticky": 20,
      "fixed": 30,
      "modalBackdrop": 40,
      "modal": 50,
      "popover": 60,
      "tooltip": 70,
      "navigation": 50,
      "mobileMenu": 190
    }
  }
}

